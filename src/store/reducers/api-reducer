import {CHANGE_MOVIES_PAGE} from "../constants/constants";
import {initialApi} from "../../services/initial-api";

const initialState = {
    ...initialApi,
}

export default function apiReducer(state = initialState, action) {
    switch (action.type) {
        case CHANGE_MOVIES_PAGE:
            return {
                ...state,
                movies: {page: action.payload},
            }
        default:
            return state;
    }
}
